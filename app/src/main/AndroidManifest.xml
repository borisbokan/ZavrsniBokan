<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="borcha.com.nekretnine">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MojaTemaCrvena">
        <activity
            android:name=".Activitys.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/MojaTemaCrvena">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        //Detalji aktivnost
        <activity android:name=".Activitys.DetaljiActivity" android:parentActivityName=".Activitys.MainActivity"/>
        //podesavanja aktivnost
        <activity android:name=".Activitys.SettingsActivity" android:parentActivityName=".Activitys.MainActivity"/>

        //podesavanja aktivnost
        <activity android:name=".Activitys.DodavanjeIspravka" android:parentActivityName=".Activitys.MainActivity"/>



    </application>

</manifest>