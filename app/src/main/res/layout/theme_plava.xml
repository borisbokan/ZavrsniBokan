<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MojaTemaPlava" parent="AppTheme">
        <item name="colorPrimary">@color/moja_plava_primary</item>
        <item name="colorPrimaryDark">@color/moja_plava_colorPrimaryDark</item>
        <item name="colorAccent">@color/moja_plava_colorAccent</item>
        <item name="android:textColorHint">@color/moja_plava_hintColor</item>
        <item name="android:background">@color/moja_plava_transp30</item>
        <item name="android:textColor">@color/moja_plava_textColor</item>
        <item name="android:listViewStyle">@style/MojaTemaPlava.lsStavkeNaziv</item>
        <item name="android:actionOverflowButtonStyle">@color/moja_plava_colorAccent</item>

    </style>


    <style name="MojaTemaPlava.lsStavkeNaziv" >
